import java.util.Scanner;

public class BMI {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);

        System.out.print("your weight (Kg): ");
        int weight = input.nextInt();
        System.out.print("\nyour height (M): ");

        double height = input.nextDouble();

        double bmi = weight / (height*height);

        if (bmi < 18.5) {
            System.out.println("your bmi is less than average.");
        }
        else if (bmi >= 18.5 && bmi <= 24.9) {
            System.out.println("you are in a normal situation");
        }
        else if (bmi >= 25 && bmi <= 29.9) {
            System.out.println("you are a little bit fat");
        }
        else if (bmi >= 30) {
            System.out.println("you are chubby");
        }


    }
}
